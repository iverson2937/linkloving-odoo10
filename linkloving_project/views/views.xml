        <template id="core_static" name="linkloving_project_assets" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <script type="text/javascript" src="/linkloving_project/static/src/js/new_gantt.js"/>
                <script type="text/javascript" src="/linkloving_project/static/src/js/dhtmlxgantt.js"/>
                <link rel="stylesheet" href="/linkloving_project/static/src/css/dhtmlxgantt.css"/>

            </xpath>
        </template>


        <record id="linkloving_res_partner_issues_button_view" model="ir.ui.view">
            <field name="name">res.partner.issues.button.view.inherit</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="project_issue.res_partner_issues_button_view"/>
            <field name="arch" type="xml">

                <xpath expr="//button[@name='%(project_issue.action_view_issues)d']" position="attributes">
                    <attribute name='invisible'>1</attribute>
                </xpath>

            </field>
        </record>
