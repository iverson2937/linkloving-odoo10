<openerp>
    <data>
        <record id="eb_order_import_form" model="ir.ui.view">
            <field name="name">EB Transfer Out</field>
            <field name="model">eb.order</field>
            <field name="inherit_id" ref="linkloving_eb.eb_order_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='create_date']" position="replace">
                    <field name="deal_date"/>
                </xpath>

                <xpath expr="//field[@name='eb_order_line_ids']/tree/field[@name='product_id']" position="after">
                    <field name="product"/>
                    <field name="price_unit"/>
                </xpath>
            </field>
        </record>
    </data>
</openerp>