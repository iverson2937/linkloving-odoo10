<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>

        <!-- Multi -->
        <record model="ir.rule" id="mrp_production_rule">
            <field name="name">mrp_process multi-company</field>
            <field name="model_id" search="[('model','=','mrp.process')]" model="ir.model"/>
            <field name="global" eval="True"/>
            <field name="domain_force">['|',('company_id','child_of',[user.company_id.id]),('company_id','=',False)]
            </field>
        </record>

        <record id="decimal_produce_price" model="decimal.precision">
            <field name="name">Produce Price</field>
            <field name="digits" eval="3"/>
        </record>


    </data>
</odoo>
