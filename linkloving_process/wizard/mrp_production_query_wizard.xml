<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id='mrp_production_query_wizard' model='ir.ui.view'>
            <field name='name'>mrp.production.query.wizard</field>
            <field name='model'>mrp.production.query.wizard</field>
            <field name='type'>form</field>
            <field name='arch' type='xml'>
                <form name='Print Reports'>
                    <group>
                        <group>
                            <field name='start_date' required='1'/>
                            <field name='end_date' required='1'/>
                        </group>
                        <group>
                            <field name="process_id"/>
                        </group>
                    </group>

                    <footer>
                        <button name='mo_query' type='object' string='Confirm' class='oe_highlight'/>
                        or
                        <button string='Cancel' class='oe_link' special='cancel'/>
                    </footer>
                </form>
            </field>
        </record>

    </data>
</openerp>