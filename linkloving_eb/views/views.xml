<openerp>
  <data>

      <record id="eb_order_tree" model="ir.ui.view">
          <field name="name">EB Transfer Out</field>
          <field name="model">eb.order</field>
          <field name="arch" type="xml">
                  <tree>
                      <field name="name" require="1"/>
                      <field name="is_finish_transfer"/>
                      <field name="state"/>
                      <field name="my_create_date"/>
                  </tree>
          </field>
      </record>

      <record id="eb_order_form" model="ir.ui.view">
          <field name="name">EB Transfer Out</field>
          <field name="model">eb.order</field>
          <field name="arch" type="xml">
                  <form>
                      <header><field name="state" widget="statusbar" />
                        <button string="确认生成出货单" type="object" name="action_confirm" states="draft"/>
                      </header>
                      <sheet>
                    <div class="oe_button_box" name="button_box">
                        <button name="action_view_delivery"
                            string="Delivery"
                            type="object"
                            class="oe_stat_button"
                            icon="fa-truck">
                        </button>
                    </div>
                          <group>

                              <group>
                                <field name="name" require="1"/>
                              </group>
                              <group>
                                <field name="create_date"/>
                              </group>
                          </group>

                          <notebook>
                              <page>
                                  <field name="eb_order_line_ids">
                                      <tree editable="bottom">
                                          <field name="product_id"/>
                                          <field name="qty"/>
                                      </tree>
                                  </field>
                              </page>
                          </notebook>
                                            </sheet>

                      </form>

          </field>
      </record>

      <record id="action_show_sale_order" model="ir.actions.act_window">
            <field name="name">Sale Orders</field>
            <field name="res_model">sale.order</field>
            <field name="type">ir.actions.act_window</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <!--<field name="domain">[('picking_type_code','=', 'incoming')]</field>-->
            <!--<field name="context">{-->
                    <!--'contact_display': 'partner_address',-->
            <!--}-->
            <!--</field>-->
            <!--<field name="search_view_id" ref="stock.view_picking_internal_search"/>-->
                <field name="help" type="html">
                <!--<field name="search_view_id" ref="sale.sale_order_view_search_inherit_sale"/>-->
              <p class="oe_view_nocontent_create">
                Click here to create a new transfer.
              </p><p>
                You can either do it immediately or mark it as Todo for future processing. Use your scanner to validate the transferred quantity quicker.
              </p>
            </field>
        </record>


