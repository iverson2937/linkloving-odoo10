      <record id="account_multi_create_sale_order_view" model="ir.ui.view">
            <field name="name">multi.create.order.form</field>
            <field name="model">multi.create.order</field>
            <field name="arch" type="xml">
                <form string="生成销售订单">
                    <p class="oe_grey">
