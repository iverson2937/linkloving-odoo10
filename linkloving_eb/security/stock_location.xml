<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="stock_location_eb_transfer" model="stock.location">
            <field name="name">EB Locatioon1</field>
            <!--<field name="location_id" ref="stock.stock_location_locations_partner"/>-->
            <field name="usage">customer</field>
            <field name="company_id"></field>
        </record>


        <record id="stock_location_eb_transfer_2" model="stock.location">
            <field name="name">工程领料仓库</field>
            <!--<field name="location_id" ref="stock.stock_location_locations_partner"/>-->
            <field name="usage">internal</field>
            <field name="company_id"></field>
        </record>


        <record id="res_partner_eb_customer" model="res.partner">
            <field name="name">电商客户</field>
            <!--<field name="supplier">1</field>-->
            <field eval="1" name="customer"/>
        </record>

        <!-- Sequences for sale.order -->
        <record id="seq_eb_order" model="ir.sequence">
            <field name="name">EB Orders</field>
            <field name="code">eb.order</field>
            <field name="prefix">EBO</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
