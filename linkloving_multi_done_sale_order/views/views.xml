<openerp>
    <data>
        <record id="multi_set_sale_order_done" model="ir.ui.view">
            <field name="name">set.sale.done.form</field>
            <field name="model">set.sale.done</field>
            <field name="arch" type="xml">
                <form string="批量出货">
                    <p class="oe_grey">
                        是否批量出货(出货数量为全部)?
                    </p>
                    <footer>
                        <button string="批量出货" name="action_ok" type="object" default_focus="1"
                                class="btn-primary"/>
                        <button string="Cancel" class="btn-default" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>


        <act_window id="action_multi_set_sale_order_done"
                    multi="True"
                    key2="client_action_multi" name="批量出货"
                    res_model="set.sale.done" src_model="sale.order"
                    view_mode="form" target="new" view_type="form"/>

    </data>
</openerp>