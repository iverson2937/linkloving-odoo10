<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>

        <record id="model_account_invoice_excel_export" model="ir.actions.server">
            <field name="name">对账单导出</field>
            <field name="type">ir.actions.server</field>
            <field name="model_id" ref="account.model_account_invoice"/>
            <field name="state">code</field>
            <field name="code">
                action = {
                   'type': 'ir.actions.act_url',
                'url': 'export/account_invoice?values=' + str(context.get('active_ids', [])),
                   'nodestroy': True
                }
            </field>
        </record>

        <record id="action_account_invoice_excel_export" model="ir.values">
            <field name="name">对账单导出</field>
            <field name="action_id" ref="linkloving_invoice_workflow.model_account_invoice_excel_export"/>
            <field name="value"
                   eval="'ir.actions.server,%d' %ref('linkloving_invoice_workflow.model_account_invoice_excel_export')"/>
            <field name="key">action</field>
            <field name="model_id" ref="account.model_account_invoice"/>
            <field name="model">account.invoice</field>
            <field name="key2">client_action_multi</field>
        </record>
    </data>
</openerp>