<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="sequence_mrp_qc_feedback" model="ir.sequence">
            <field name="name">MRP Qc Feedback</field>
            <field name="code">mrp.qc.feedback</field>
            <field name="prefix">QCF/%(year)s%(month)s%(day)s</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">4</field>
        </record>

        <record model="ir.rule" id="mrp_qc_feedback_rule">
            <field name="name">品检单multi-company</field>
            <field name="model_id" search="[('model','=','mrp.qc.feedback')]" model="ir.model"/>
            <field name="global" eval="True"/>
            <field name="domain_force">['|',('company_id','child_of',[user.company_id.id]),('company_id','=',False)]
            </field>
        </record>
    </data>
</odoo>
