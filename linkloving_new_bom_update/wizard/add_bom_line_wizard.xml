<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>


        <record id="add_bom_line_wizard_form_view" model="ir.ui.view">
            <field name="name">add.bom.line.wizard.form</field>
            <field name="model">add.bom.line.wizard</field>
            <field name="arch" type="xml">
                <form string="">
                    <group>
                        <field name="product_id"/>
                        <field name="qty"/>
                        <field name="product_specs"/>
                    </group>
                    <footer>
                        <button string="确定" name="action_post" type="object" default_focus="1"
                                class="btn-primary"/>
                        <button string="取消" class="btn-default" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

    </data>
</openerp>
