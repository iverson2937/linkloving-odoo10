<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="sequence_work_order_number" model="ir.sequence">
            <field name="name">work order number</field>
            <field name="code">work.order.number</field>
            <field name="prefix">WO%(y)s%(month)s%(day)s</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">4</field>
        </record>
    </data>
</odoo>
