<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="status_light_type" model="auto.plan">
            <field name="name">状态灯</field>
            <field name="type">status_light</field>
        </record>
        <record id="stock_picking_type" model="auto.plan">
            <field name="name">入库单</field>
            <field name="type">stock_picking</field>
        </record>
        <record id="waiting_file_type" model="auto.plan">
            <field name="name">采购订单</field>
            <field name="type">waiting_file</field>
        </record>


        <record id="sequence_mrp_report" model="ir.sequence">
            <field name="name">mrp.report</field>
            <field name="code">mrp.report</field>
            <field name="prefix">MR/%(year)s%(month)s%(day)s</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">4</field>
        </record>
    </data>
</odoo>
